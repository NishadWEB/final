{
  "name": "preliminary-diagnosis-webapp",
  "version": "1.0.0",
  "description": "Medical diagnosis platform connecting patients and doctors",
  "main": "backend/server.js",
  "scripts": {
    "start": "node backend/server.js",
    "dev": "nodemon backend/server.js",
    "setup-db": "node backend/config/setup.js",
    "seed-db": "node backend/config/seed.js",
    "ml-service": "cd ml_service && python model.py",
    "install-deps": "npm install && cd ml_service && pip install -r requirements.txt",
    "full-setup": "npm run install-deps && npm run setup-db && npm run seed-db"
  },
  "dependencies": {
    "express": "^4.18.2",
    "ejs": "^3.1.9",
    "pg": "^8.11.3",
    "bcryptjs": "^2.4.3",
    "express-session": "^1.17.3",
    "body-parser": "^1.20.2",
    "dotenv": "^16.3.1",
    "axios": "^1.5.0",
    "socket.io": "^4.7.2",
    "moment": "^2.29.4",
    "express-validator": "^7.0.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  },
  "keywords": ["NISHAD", "SHIFAN", "MUZAMIL", "UDAIF"],
