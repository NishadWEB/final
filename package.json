{
  "name": "preliminary-diagnosis-webapp",
  "version": "1.0.0",
  "description": "Medical diagnosis platform connecting patients and doctors",
  "main": "backend/server.js",
  "scripts": {
    "start": "node backend/server.js",
    "dev": "nodemon backend/server.js",
    "setup-db": "node backend/config/setup.js",
    "seed-db": "node backend/config/seed.js",
    "ml-service": "cd ml_service && python model.py",
    "ml-health": "powershell -Command \"(Invoke-WebRequest -Uri http://localhost:5000/health -UseBasicParsing -TimeoutSec 5).Content\"",
    "install-deps": "npm install && cd ml_service && python -m venv .venv && .venv\\Scripts\\python -m pip install --upgrade pip && .venv\\Scripts\\python -m pip install -r requirements.txt",
    "wait-ml": "powershell -File scripts/wait-for-ml.ps1",
    "full-setup": "npm run install-deps && npm run setup-db && npm run seed-db"
  },
  "dependencies": {
    "axios": "^1.5.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.3",
    "dotenv": "^16.6.1",
    "ejs": "^3.1.9",
    "express": "^4.21.2",
    "express-session": "^1.18.2",
    "express-validator": "^7.0.1",
    "http": "^0.0.1-security",
    "moment": "^2.29.4",
    "path": "^0.12.7",
    "pg": "^8.11.3",
    "socket.io": "^4.8.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  },
  "keywords": [
    "NISHAD",
    "SHIFAN",
    "MUZAMIL",
    "UDAIF"
  ],
  "author": "MediDiag Team",
  "license": "MIT"
}
